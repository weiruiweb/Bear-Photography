/**index.wxss**/
@import "../../wxParse/wxParse.wxss";
page{
	width:100%;
	min-height:100%;
	background:#f5f5f5;
}

/***************menu****************/
.menu{
	width:100%;
	height:210rpx;
}
.menu_item{
	width:110rpx;
	height:110rpx;
	border-radius:50%;
	overflow:hidden;
	position:relative;
}
.menu_item image{
	width:100%;
	height:100%;
}
.menu_box{
	width:100%;
	height:100%;
	position: absolute;
	left:0;
	top:0;
	text-align: center;
	line-height:110rpx;
}
.store_img{
	width:100%;
	height:330rpx;
	margin-bottom:24rpx;
}
.store_img image{
	width:100%;
	height:100%;
}