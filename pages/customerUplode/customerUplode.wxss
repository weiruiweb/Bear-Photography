page{
	min-height:100%;
	width:100%;
	background:#fff;
}
.submit{
	width:100%;
}

.upload_img{
	flex-wrap:wrap;
	border-bottom:solid 1px #d6d6d6;
}
.upload_item{
	width:120rpx;
	height:120rpx;
	border:dashed 1px #e5e5e5;
	text-align:center;
	line-height:120rpx;
	margin:0 10rpx;
}
.upload_item image{
	width:100%;
	height:100%;
}
.upload_img{
	border:none;
}
.uplode_btn{
	width:100%;
	height:84rpx;
	line-height:84rpx;
}
.uplode_btn image{
	width:24rpx;
	height:24rpx;
	margin-right:20rpx;
}
.input_before1 {
	margin-right:2%;
	width:16%;
}
.input_list1{
	width:92%;
	height:90rpx;
	margin:0 auto;
	border-bottom:solid 1px #f6f6f6;
}
.input_list1 .input_before{
	width:22%;
}
.input_list1 input{
	width:76%;
	padding-left:2%;
	height:100%;
	border-radius:16rpx;
}
.user_icon1{
	float:right;
}
.user_icon1 image{
	width:14rpx;
	height:14rpx;
}
.upload{
	padding-top:100rpx;
	padding-bottom:60rpx;
}